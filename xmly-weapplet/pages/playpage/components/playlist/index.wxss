@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-bofang:before {
  content: "\e616";
}

.icon-add-sy:before {
  content: "\e501";
}

.icon-zantingbofang:before {
  content: "\e70d";
}

.icon-bofang1:before {
  content: "\e8aa";
  color: #f86442;
}


.play-list-top{
  display: flex;
  text-align: center;
  margin-top: 10rpx;
  border-bottom: #eee 5rpx solid;
}
.play-list-tab{
  flex: 1;
  font-size: 14px;
  margin: 0 60rpx;
}
.active-tab{
  border-bottom: 5rpx solid #f86442;
}
.active-item{
  color: #f86442;
}

.play-list-control{
  font-size: 14px;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;
  display: flex;
  border-bottom: #eee 5rpx solid;
}
.play-control-all{
  width: 70%;
}
.play-control-sort{
  width: 15%;
  text-align: center;
}
.play-control-check{
  width: 15%;
  text-align: center;
}

.play-item-cont{
  display: flex;
  justify-content: center;
  align-items: center;
}

.play-list-item{
  padding: 20rpx;
  border-bottom: #eee 5rpx solid;
}

.play-item-index{
  font-size: 13px;
  font-weight: 600;
  color: #999;
  width: 15%;
  text-align: center;
}

.play-item-title{
  font-size: 13px;
  width: 70%;
}

.play-item-control{
  width: 15%;
  text-align: center;
}
.size-big{
  font-size: 25px;
  color: #999;
}

.play-item-count{
  padding: 15rpx 0 0 80rpx;
  font-size: 12px;
  color: #999;
}