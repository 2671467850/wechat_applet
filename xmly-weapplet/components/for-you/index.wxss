@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-erji:before {
  content: "\e605";
}

.for-you-name{
  margin-top: 25rpx;
  font-size: 38rpx;
}

.for-you-item{
  display: flex;
  padding: 30rpx 0;
  border-bottom: solid 1px #eee;
}

.for-you-img{
  width: 25%;
  height: 25vw;
  margin-right: 20rpx;
}

.item-img{
  width: 100%;
  height: 100%;
  vertical-align: top;
  border-radius: 10%;
}

.for-you-word{
  width: 75%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.for-you-title{
  font-size: 35rpx;
}

.for-you-point{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 25rpx;
  background: #eee;
  padding: 0 20rpx;
  border-radius: 10rpx;
  color: #666;
  width: 350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.for-you-count{
  font-size: 25rpx;
  color: #666;
}